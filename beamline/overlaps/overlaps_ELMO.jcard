{
	# Run check overlaps for beamline ELMO

	# verbosities
	"verbosity": 1,
	"gsystemv": 1,
	"g4systemv": 1,

	# nthreads must be 1 to run checkOverlaps
	"nthreads": 1,

	"+gsystem": [
		{ "system": "./clas12ForwardCarriage", "factory": "text", "variation": "original" },
		{ "system": "./beamline", "factory": "text", "variation": "ELMO" }
	],

	"checkOverlaps": 1
}
